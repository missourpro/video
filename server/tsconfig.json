{
  "compilerOptions": {

    "allowSyntheticDefaultImports": true,
    //"declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es2015",
    "types": [
      "node",
      "electron"
    ],
    "typeRoots": [
      "../node_modules/@types"
    ],
    "noImplicitAny": false
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  },
  "disableWarnings": false,
  "fast": true
}
