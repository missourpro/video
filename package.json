  "author": "Mohammed EL HADROUBI",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "postinstall": "install-app-deps",
    "install:post": "npm run install:post:test && yarn run install:post:server",
    "install:post:server": "ffbinaries --output=./node_modules/ffmpeg",
    "install:post:test": "webdriver-manager update",
    "test": "npm run test:server",
    "test:server": "npm run test:server:unit && npm run test:server:integration && npm run test:server:e2e",
    "test:client:unit": "ng test",
    "test:client:e2e": "protractor",
    "test:server:unit": "jasmine JASMINE_CONFIG_PATH=server/test/unit/jasmine.json",
    "test:server:integration": "node ./server/test/main.js --suite integration",
    "test:server:e2e": "node ./server/test/main.js --suite e2e",
    "test:client:coverage": "ng test --code-coverage",
    "clean": "ionic-app-scripts clean",
    "build:client": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "start:client": "ionic-app-scripts serve",
    "start:server": "electron .",
    "electron dist": "electron .",
    "clear:osx": "rm -rf *-darwin-x64; rm -rf ./platforms/*-mas-x64",
    "clear:win": "rm -rf *-win32-ia32; rm -rf ./platforms/*-win32-x64",
    "clear:nix": "rm -rf *-linux-ia32; rm -rf ./platforms/*-linux-x64",
    "build:osx": "npm run clear:osx && cp package.json www/ && electron-packager ./www/ --platform=darwin,mas --arch=x64 --asar --out=platforms ",
    "build:win": "npm run clear:win && cp package.json www/ && electron-packager ./www/ --platform=win32 --arch=ia32,x64 --asar --out=platforms ",
    "build:nix": "npm run clear:nix && cp package.json www/ && electron-packager ./www/ --platform=linux --arch=ia32,x64 --asar --out=platforms "
  },
